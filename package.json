{
  "name": "shoka-validate",
  "version": "0.0.0",
  "description": "Wizz Form validation",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./valibot": {
      "import": "./dist/valibot.js",
      "require": "./dist/valibot.cjs",
      "types": "./dist/valibot.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "./LICENSE",
    "./dist"
  ],
  "scripts": {
    "dev:playground": "pnpm run -C playground/vue3 dev",
    "build": "vue-tsc && vite build"
  },
  "keywords": [
    "form",
    "validation"
  ],
  "author": "Ákos Stégner",
  "license": "MIT",
  "dependencies": {
    "valibot": "^0.30.0",
    "vue": "^3.4.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vueuse/core": "^10.9.0",
    "lodash-es": "^4.17.21",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vue-tsc": "^2.0.4"
  }
}
